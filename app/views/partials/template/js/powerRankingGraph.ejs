      [13,  4.8,  6.3,  3.6, 22],
      [14,  4.2,  6.2,  3.4, 24]
    ]);

    var options = {
      chart: {
        title: 'Average Power Rankings',
        subtitle: 'from Reddit NFL'
      },
      width: 900,
      height: 500,
      colors: ['#ff9900', '#00ffff', '#ffff00', '#000000']
